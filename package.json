{
  "type": "module",
  "name": "htm",
  "version": "0.0.0",
  "description": "Convert .txt to HTML2, autolinked.",
  "main": "index.ts",
  "scripts": {
    "test": "NODE_NO_WARNINGS=1 c8 tap",
    "prepare": "husky install"
  },
  "tap": {
    "node-arg": [
      "--loader=ts-node/esm"
    ],
    "coverage": false,
    "ts": false
  },
  "keywords": [
    "html2",
    "rfc1866",
    "txt",
    "autolink"
  ],
  "repository": "weakish/htm",
  "bugs": {
    "url": "https://github.com/weakish/htm/issues"
  },
  "author": {
    "name": "Jang Rush",
    "email": "weakish@gmail.com",
    "url": "https://mmap.page"
  },
  "license": "0BSD",
  "dependencies": {
    "linkify-string": "^3.0.4",
    "linkifyjs": "^3.0.5"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/node": "^18.7.16",
    "@types/tap": "^15.0.7",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "c8": "^7.12.0",
    "eslint": "^8.23.0",
    "husky": "^8.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
